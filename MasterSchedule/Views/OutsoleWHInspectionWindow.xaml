<Window x:Class="MasterSchedule.Views.OutsoleWHInspectionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Master Schedule - Outsole WH Inspection" Height="500" Width="800">
    <Grid Margin="2">
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="ProductNo: " VerticalAlignment="Bottom"/>
            <TextBox x:Name="txtProductNo"/>
        </StackPanel>
    </Grid>
</Window>
